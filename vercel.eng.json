{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/eng"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)\\.(js|css|ico|png|jpg|jpeg|svg|webp|woff2?)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
