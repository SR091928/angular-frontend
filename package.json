    "@angular-eslint/template-parser": "^20.5.1",
    "@angular/cli": "^18.2.14",
    "@angular/compiler-cli": "^18.2.14",
    "@nx/angular": "^18.2.0",
    "@nx/cypress": "21.5.3",
    "@nx/eslint": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@types/jest": "^29.5.5",
