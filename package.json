{
  "name": "angular-frontend",
  "version": "1.0.0",
  "description": "Angular frontend application with Jest and Cypress configured for testing.",
  "scripts": {
    "build": "nx build angular-frontend",
    "serve": "nx serve angular-frontend",
    "build:eng": "rimraf dist/eng && nx build angular-frontend --configuration eng --base-href=/",
    "serve:eng": "nx serve angular-frontend --configuration=eng",
    "build:prod": "rimraf dist/prod && nx build angular-frontend --configuration production --base-href=/",
    "serve:prod": "nx serve angular-frontend --configuration=production",
    "test": "nx test angular-frontend",
    "lint": "nx lint angular-frontend",
    "test:coverage": "npm run test --coverage --watch=true",
    "clean": "rimraf dist coverage"
  },
  "engines": {
    "node": "20.*",
    "npm": "10.*"
  },
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular-devkit/core": "^18.2.7",
    "@angular-devkit/schematics": "^18.2.7",
    "@angular-eslint/builder": "^18.3.0",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/schematics": "^18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.0",
    "@nx/angular": "21.5.3",
    "@nx/eslint": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@schematics/angular": "^18.2.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/utils": "^7.18.0",
    "cypress": "^13.0.0",
    "eslint": "^8.57.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-preset-angular": "^15.0.0",
    "nx": "21.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "nx": {}
}
