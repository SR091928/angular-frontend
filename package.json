{
  "name": "@sr091928/angular-frontend",
  "version": "1.0.0",
  "description": "Angular frontend application with Jest and Cypress configured for testing.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "local": "npm run clean && npm run build && npm run serve",
    "build": "nx build angular-frontend",
    "serve": "nx serve angular-frontend --open",
    "build:staging": "rimraf dist && nx build angular-frontend --configuration=staging",
    "serve:staging": "nx serve angular-frontend --configuration=staging",
    "build:prod": "rimraf dist && nx build angular-frontend --configuration=production",
    "serve:prod": "nx serve angular-frontend --configuration=production",
    "lint": "nx lint angular-frontend",
    "lint:fix": "nx lint angular-frontend --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json,md}\"",
    "test": "nx test angular-frontend",
    "e2e": "nx e2e angular-frontend"
  },
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.5",
