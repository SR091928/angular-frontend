{
  "version": 2,
  "name": "sr-angular-frontend-prod",
  "public": true,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ],
  "env": {
    "ENV": "prod"
  },
  "github": {
    "enabled": true,
    "silent": false
  }
}
